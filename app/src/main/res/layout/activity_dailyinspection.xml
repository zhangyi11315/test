<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/llMain"
    android:background="@color/free_ui_glory_bg">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_1"
        android:orientation="horizontal"
        android:background="@color/free_ui_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="批号"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_blue_sky_theme_color"/>

        <TextView
            android:id="@+id/lot_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_dark_gold_text_black" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_1"
        android:orientation="horizontal"
        android:background="@color/free_ui_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="批号数量"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_blue_sky_theme_color"/>

        <TextView
            android:id="@+id/lotNumber_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_dark_gold_text_black" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_1"
        android:orientation="horizontal"
        android:background="@color/free_ui_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="批号状态"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_blue_sky_theme_color"/>

        <TextView
            android:id="@+id/lotState_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_dark_gold_text_black" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_1"
        android:orientation="horizontal"
        android:background="@color/free_ui_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="物料名称"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_blue_sky_theme_color"/>

        <TextView
            android:id="@+id/materielName_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_dark_gold_text_black" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_1"
        android:orientation="horizontal"
        android:background="@color/free_ui_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="物料料号"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_blue_sky_theme_color"/>

        <TextView
            android:id="@+id/materielNumber_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_dark_gold_text_black" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_1"
        android:orientation="horizontal"
        android:background="@color/free_ui_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="规格"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_blue_sky_theme_color" />

        <TextView
            android:id="@+id/pecifications_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_dark_gold_text_black" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_1"
        android:orientation="horizontal"
        android:background="@color/free_ui_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="型号"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_blue_sky_theme_color"/>

        <TextView
            android:id="@+id/model_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_dark_gold_text_black" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_1"
        android:orientation="horizontal"
        android:background="@color/free_ui_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="供应商"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_blue_sky_theme_color"/>

        <TextView
            android:id="@+id/supplier_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_dark_gold_text_black" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_1"
        android:orientation="horizontal"
        android:background="@color/free_ui_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="有效期"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_blue_sky_theme_color"/>

        <TextView
            android:id ="@+id/validity_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/free_ui_dp_12"
            android:textColor="@color/free_ui_dark_gold_text_black" />

    </LinearLayout>

</LinearLayout>